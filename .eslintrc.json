{
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "double"],
    "linebreak-style": [2,"unix"],
    "semi": [2, "always"],
    "comma-dangle": [0, "always"]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "extends": "eslint:recommended"
}
